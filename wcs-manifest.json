  "image": "wcs-logo.png",
  "searchTags": ["map","sta","mobility"],
  "dist": {
    "basePath": "dist",
    "files": [
      "map_widget.min.js"
    ]
  },
  "configuration": {
    "tagName": "e-mobility-map-widget",
    "options": [
      {
        "key": "language",
        "type": "select",
        "label": "Language",
        "required": false,
        "options": {
          "values": ["en", "de", "it", "nl", "cs", "pl", "fr", "ru"],
          "default": "en"
        }
      },
      {
        "key": "logo",
        "type": "select",
        "label": "Address of an icon, that should be shown in the lower left corner",
        "required": false,
        "options": {
          "default": "https://www.greenmobility.bz.it/typo3conf/ext/interho/Resources/Public/Img/logo.png"
        }
      }
    ]
  }
}
